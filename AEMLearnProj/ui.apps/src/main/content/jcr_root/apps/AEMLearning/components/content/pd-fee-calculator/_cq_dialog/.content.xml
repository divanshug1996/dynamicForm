<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="pd fee"
    sling:resourceType="cq/gui/components/authoring/dialog"
    HelpPath="en/cq/current/wcm/default_components.html#Columns">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <variationTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Variation"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <formTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Please enter the form heading"
                                fieldLabel="Form Heading"
                                name="./formTitle"/>
                            <formDescription
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textarea"
                                fieldDescription="Please enter the form description"
                                fieldLabel="Form Description"
                                name="./formDescription"/>
                            <formType
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                defaultValue="commercial"
                                fieldDescription="Please select the form type"
                                fieldLabel="Type of form"
                                name="./formType">
                                <items jcr:primaryType="nt:unstructured">
                                     <default
                                        jcr:primaryType="nt:unstructured"
                                        text="Please select form type"
                                        value="default"/>
                                    <commercial
                                        jcr:primaryType="nt:unstructured"
                                        text="Commercial"
                                        value="commercial"/>
                                    <newhome
                                        jcr:primaryType="nt:unstructured"
                                        text="New Homes"
                                        value="new_home"/>
                                    <planningApp
                                        jcr:primaryType="nt:unstructured"
                                        text="Planning Application"
                                        value="planningApp"/>
                                    <multiFammilyLowRise
                                        jcr:primaryType="nt:unstructured"
                                        text="Multi Fammily Low Rise"
                                        value="low_rise"/>
                                    <multiFammilyHighRise
                                        jcr:primaryType="nily High Rise"
                                        value="high_rist:unstructured"
                                        text="Multi Famme"/>
                                    <demolition
                                        jcr:primaryType="nt:unstructured"
                                        text="Demolition"
                                        value="demolition"/>
                                    <hotels
                                        jcr:primaryType="nt:unstructured"
                                        text="Hotels"
                                        value="hotels"/>
                                    <careFacilities
                                        jcr:primaryType="nt:unstructured"
                                        text="Care Facilities"
                                        value="care_homes"/>
                                    <wareHouses
                                        jcr:primaryType="nt:unstructured"
                                        text="Warehouses"
                                        value="warehouses"/>
                                </items>
                            </formType>
                        </items>
                    </column>
                </items>
            </variationTab>
        </items>
    </content>
</jcr:root>
